{
  "name": "notes-api",
  "version": "2.0.0-alpha",
  "scripts": {
    "dev": "nodemon app.js",
    "start": "node app.js",
    "changelog": "auto-changelog && git add CHANGELOG.md"
  },
  "auto-changelog": {
    "template": "changelog-template.hbs",
    "compareUrl": "https://github.com/Lowe-Man/notes-api/compare/{from}...{to}",
    "unreleased": false,
    "commitLimit": false,
    "breakingPattern": "BREAKING CHANGE:",
    "ignoreCommitPattern": "^test:.+|docs:.+",
    "hideCredit": true,
    "replaceText": {
      "feat:": "",
      "fix:": "",
      "chore:": "",
      "refactor:": "",
      "style:": "",
      "perf:": ""
    }
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-defaults": "^5.0.0",
    "express": "^4.18.1",
    "express-validator": "^6.13.0",
    "ioredis": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.0.6",
    "sequelize": "^6.7.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "nodemon": "^2.0.13",
    "sequelize-cli": "^6.3.0"
  }
}
